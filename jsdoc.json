{
    "tags": {
        "allowUnknownTags": false
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "meta": {
            "title": "Scenario.js - API",
            "description": "DESCRIPTION",
            "keyword": "Scene.js, Scene, Scenario, Animation"
        }
    }
}